	// Loops 1
	public static void loop1() {
		for (int i = 1; i <= 10; i++) {
			if (i == 10) {
				System.out.print(i);
			}else {
				System.out.print(i + ", ");
			}
			
		}
	}
	
	// Loops 2
	public static void loop2() {
		
//		for (int i = 1, j = 100; i <= 7  && j > 0; i++, j--) {
